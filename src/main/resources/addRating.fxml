<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import org.controlsfx.control.Rating?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddRatingController">

    <children>
        <Text layoutX="218.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0" text="what is your rating ?" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <Rating fx:id="Ratingvalue" layoutX="212.0" layoutY="176.0" prefHeight="14.0" prefWidth="208.0" rating="3.0" />
        <Button layoutX="153.0" layoutY="288.0" mnemonicParsing="false" onAction="#submitrating" prefHeight="38.0" prefWidth="90.0" text="Submit">
            <font>
                <Font name="Arial Bold" size="15.0" />
            </font>
        </Button>
        <Button fx:id="cancelbtn" layoutX="388.0" layoutY="288.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="90.0" text="Cancel">
            <font>
                <Font name="Arial Bold" size="15.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
