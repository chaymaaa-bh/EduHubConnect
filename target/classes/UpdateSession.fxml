        <Label layoutX="157.0" layoutY="75.0" text="Name" />
        <TextField fx:id="upnmses" layoutX="226.0" layoutY="71.0" promptText="Enter new session's name" />
        <Label layoutX="143.0" layoutY="130.0" text="Description" />
        <TextField fx:id="updescSes" layoutX="226.0" layoutY="126.0" promptText="Enter new description" />
      <Label layoutX="162.0" layoutY="183.0" text="Link" />
      <TextField fx:id="newsessionLinkId" layoutX="226.0" layoutY="179.0" promptText="Enter new link" />
    </children>
</AnchorPane>
